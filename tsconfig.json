{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["./components/*"],
      "@/components/*": ["./components/*"],
      "@/data/*": ["./app/data/*"],
      "@/lib/*": ["./lib/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.js", "**/*.tsx", ".next/types/**/*.ts", "components/HubSpotForm.js", "utils/useWindowDimensions.jsx", "utils/useLocalStorage.jsx", "components/NewBubble/index.jsx", "components/Bubble/index.jsx"],
  "exclude": ["node_modules"]
}
